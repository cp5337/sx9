schema = "RFC-9102"
owner = "Synaptix9 Onboarding Engine"
created_at = "auto:now"
hash = "auto:trivariate"     # SCH + UUID + CUID (RFC-9100)
validated = false

[execution]
mode = "deterministic"
strict = true
max_time_ms = 5000
approved = false
requires_human_gate = true

description = """
This executable document initializes the SX9 deterministic onboarding pipeline.
It drives the SPIRES → Needle → RepoScan → DataSpine → ANN → GLAF → ISO-L2 → NATS → Neo4j chain.
It is the authoritative initialization blueprint for SX9 Antigravity onboarding.
"""

[input]
accepted_formats = ["prompt", "toml"]
